<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <title>Manchester United Matches</title>
</head>
<body>
<div class="container">

    <div class="container">
        <div class="row">
            <div class="column">
                <h1 class="mt-3">Upcoming Games</h1>
            </div>
        </div>
        <% fixtures.forEach(({kickOff, date, onSite,vs})=>{ %>

            <div class="card mt-2" >
                <div class="card-body">
                    <h5 class="card-title"><%= date %></h5>
                    <h6 class="card-subtitle mb-2 text-muted">vs <%= vs %></h6>
                    <span class="card-link">On Site <%= onSite %></span>
                    <span class="card-link">KO <%= kickOff %></span>
                </div>
            </div>
            <% }) %>
    </div>

</div>

</body>
</html>
